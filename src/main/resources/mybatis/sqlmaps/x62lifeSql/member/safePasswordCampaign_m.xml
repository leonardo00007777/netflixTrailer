<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="safePasswordCampaign">
<!--    '프로젝트: 자연이랑 모바일 웹-->
<!--    '프로그램: 비밀번호 변경 캠페인-->

<!--    '20170302   sykim   처음작성-->
<!--    '20170313   hwkim   레이아웃 조정-->
    <select id="getPasswordChangeHistory">
        /*getPasswordChangeHistory 비밀번호 변경 이력 가져오기*/
        SELECT COUNT(1) cnt
        FROM changefieldhistory
        <![CDATA[
         WHERE DATEDIFF(d,date_mempw,GETDATE()) <= 180
        ]]>
           AND memcd = #{strLoginMemCd}
    </select>
</mapper>